{
  "name": "vk-filter-search",
  "version": "1.0.1",
  "description": "Support Filtering Search of keyword, post-type and taxonomy.",
  "main": "build/index.js",
  "scripts": {
    "build": "wp-scripts build inc/vk-filter-search/package/assets/_js/block.js --output-path=inc/vk-filter-search/package/assets/js/ && npm run translate",
    "start": "wp-scripts start inc/vk-filter-search/package/assets/_js/block.js --output-path=inc/vk-filter-search/package/assets/js/ &&  npm run translate",
    "check-engines": "wp-scripts check-engines",
    "check-licenses": "wp-scripts check-licenses",
    "lint:css": "wp-scripts lint-style inc/vk-filter-search/package/assets/css/ --fix",
    "lint:js": "wp-scripts lint-js inc/vk-filter-search/package/assets/_js/ --fix",
    "make-pot": "wp i18n make-pot ./ --domain=vk-filter-search",
    "translate": "./node_modules/.bin/po2json ./languages/vk-filter-search-ja.po ./languages/vk-filter-search-ja-vk-filter-search-build-js.json -f jed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vektor-inc/vk-filter-search.git"
  },
  "author": "Vektor, Inc.",
  "license": "GPL-2.0-or-later",
  "bugs": {
    "url": "https://github.com/vektor-inc/vk-filter-search/issues"
  },
  "homepage": "https://github.com/vektor-inc/vk-filter-search#readme",
  "dependencies": {
    "@wordpress/postcss-plugins-preset": "^1.3.1"
  },
  "devDependencies": {
    "@wordpress/scripts": "^12.1.1",
    "@wordpress/server-side-render": "^1.16.1",
    "@wordpress/env": "^1.6.0",
    "@wordpress/i18n": "^3.14.0",
    "po2json": "^0.4.5"
  }
}